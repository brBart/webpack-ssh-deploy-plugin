  "version": "0.0.1",
  "description": "A webpack plugin for deploy assets to remote server over sftp",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive ./src/**/*.spec.ts",
